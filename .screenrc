autodetach on
defscrollback 50000
defmonitor on
escape ^t^t
shell zsh
startup_message off
vbell off
bind w windowlist -b
#hardstatus alwayslastline "[%02c] %`%-w%{=b bw}%n %t%{-}%+w"
hardstatus alwayslastline
hardstatus string '%{= kW}%-Lw%{= KW}%50>%n%f* %t%{= kW}%+Lw%< %{= kG}%-=%y/%m/%d %c:%s%{-}'
termcapinfo xterm* ti@:te@

